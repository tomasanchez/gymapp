<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="tosa8.my_gym.controller.SportsList"
	xmlns:html="http://www.w3.org/1999/xhtml">
			<Page title="Sports" showNavButton="true"
		navButtonPress=".onNavBack"
		class="sapUiResponsiveContentPadding">
				<content>
					<List id="sportsSet" items="{/DeportesSet}" noDataText="Loading clients...">
						<items>
							<StandardListItem title="{IdDep} - ${Costo}" description="ID. {IdClte} - Prof. {Profe}" info="Days: {Horarios}"/>
						</items>
					</List>
				</content>
			</Page>
</mvc:View>